{
  "background_page": "background.html",
  "content_scripts": [
    {
      "js": [
        "js/tab-content-script.js"
      ], 
      "matches": [
        "http://*/*"
      ]
    }
  ],
  "page_actions": [
    {
        "id": "tag_page",
        "name": "Tag this page on Delicious",
        "icons": ["images/tag_blue_16.png", "images/tag_gray_16.png"]
    }
  ],
  "permissions": [
        "tabs",
        "https://api.del.icio.us/"
  ],
  "name": "Del.icio.us Tools",
  "version": "0.5",
  "description": "Del.icio.us toolbar for Chrome",
  "toolstrips": [
    "main_toolstrip.html"
  ]
}
